<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="poo.consultorio.DetalleDefinicionAgenda" table="DetalleDefinicionAgenda">
      <id name="id" column="id_detalle_def_agenda">
          <generator class="increment"/>
      </id>
      <property name="diaSemana" column="dia_semana" />
      <property name="duracionIntervalo" column="duracion_intervalo" />
      <property name="horaInicio" column="hora_inicio" type="java.util.Date" />
      <property name="horaFin" column="hora_fin" type="java.util.Date" />
      <property name="horaInicioIntervalo" column="hora_inicio_intervalo" type="java.util.Date" />
    </class>
</hibernate-mapping>
